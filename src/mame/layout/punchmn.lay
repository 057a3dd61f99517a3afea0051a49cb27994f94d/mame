		<bezel name="right_bottom_lamp" element="red_lamp">
			<bounds x="700" y="400" width="25" height="25" />
		</bezel>
		
		<!-- control panel lamps -->
		<bezel name="select_left_right" element="yellow_lamp">
			<bounds x="285" y="500" width="50" height="25" />
		</bezel>

		<bezel name="start_lamp" element="red_lamp">
			<bounds x="345" y="500" width="50" height="25" />
		</bezel>
		
		<bezel name="select_left_right" element="yellow_lamp">
